package rules.rules_1

import com.ftn.sbnz.model.examples_1.Message

global java.lang.Integer newGlobal;

rule "Hello World 2"
    agenda-group "promotions"
    when
        $m: Message(status == Message.ZDRAVO, myMessage: message)
    then
        System.out.println("Hello from Hello World 2");
        modify($m){setMessage("Goodbye from Hello World 2"), setStatus( Message.GOODBYE) };
end